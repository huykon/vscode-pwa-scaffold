{
    "name": "vscode-pwa-scaffold",
    "publisher": "huykon225",
    "displayName": "PWA Studio Scaffold Snippets",
    "description": "Magento PWA Studio snippets by Mason Tran",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.72.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:vscode-pwa-scaffold.helloWorld"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vscode-pwa-scaffold.helloWorld",
